          <source>test.xsl</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>test.xml</source>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>The following code uses the <bpt id="p1">**</bpt>test.xsl<ept id="p1">**</ept> style sheet and <bpt id="p2">**</bpt>test.xml<ept id="p2">**</ept> input data.</source>
