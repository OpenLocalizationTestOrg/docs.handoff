        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>Option available since .NET Core 2.1 SDK.</source>
        </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>Specifies the server URL.</source>
        </trans-unit>
        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>This option is required unless <ph id="ph1">`DefaultPushSource`</ph> config value is set in the NuGet config file.</source>
        </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source>The API key for the symbol server.</source>
        </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>Specifies the symbol server URL.</source>
        </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>Specifies the timeout for pushing to a server in seconds.</source>
        </trans-unit>
        <trans-unit id="132" translate="yes" xml:space="preserve">
          <source>Defaults to 300 seconds (5 minutes).</source>
        </trans-unit>
        <trans-unit id="133" translate="yes" xml:space="preserve">
          <source>Specifying 0 (zero seconds) applies the default value.</source>
