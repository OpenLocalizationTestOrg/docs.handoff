          <source>Basic128</source>
        </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>Use Aes128 encryption, Sha1 for message digest, and Rsa-oaep-mgf1p for key wrap.</source>
        </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>Basic192</source>
        </trans-unit>
        <trans-unit id="132" translate="yes" xml:space="preserve">
