        <trans-unit id="101" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>Inheriting from 'System.<ph id="ph1">&lt;classname&gt;</ph>'</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>is not valid</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Inheriting from 'System.<ph id="ph1">\&lt;</ph>classname&gt;' is not valid</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>A class cannot inherit from <ph id="ph1">`System.Array`</ph>, <ph id="ph2">`System.Delegate`</ph>, <ph id="ph3">`System.Enum`</ph>, or <ph id="ph4">`System.ValueType`</ph>.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source><bpt id="p1">**</bpt>Error ID:<ept id="p1">**</ept> BC30015</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>To correct this error</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Remove the <ph id="ph1">`Inherits`</ph> statement or change it to inherit from a valid base class.</source>
