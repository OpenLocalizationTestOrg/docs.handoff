        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>ICorDebugProcess5::GetTypeFields Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Provides information about the fields that belong to a type.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] The identifier of the type whose field information is retrieved.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[in] The number of <bpt id="p1">[</bpt>COR_FIELD<ept id="p1">](../../../../docs/framework/unmanaged-api/debugging/cor-field-structure.md)</ept> objects whose field information is to be retrieved.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>[out] An array of <bpt id="p1">[</bpt>COR_FIELD<ept id="p1">](../../../../docs/framework/unmanaged-api/debugging/cor-field-structure.md)</ept> objects that provide information about the fields that belong to the type.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>[out] A pointer to the number of <bpt id="p1">[</bpt>COR_FIELD<ept id="p1">](../../../../docs/framework/unmanaged-api/debugging/cor-field-structure.md)</ept> objects included in <ph id="ph1">`fields`</ph>.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">`celt`</ph> parameter, which specifies the number of fields whose field information the method uses to populate <ph id="ph2">`fields`</ph>, should correspond to the value of the <ph id="ph3">`COR_TYPE_LAYOUT::numFields`</ph> field.</source>
        </trans-unit>
