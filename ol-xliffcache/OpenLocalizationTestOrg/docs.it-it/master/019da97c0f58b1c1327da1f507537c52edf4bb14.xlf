        <trans-unit id="101" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>Conversion from '<ph id="ph1">&lt;type1&gt;</ph>' to '<ph id="ph2">&lt;type2&gt;</ph>' cannot occur in a constant expression</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Conversion from '<ph id="ph1">\&lt;</ph>type1&gt;' to '<ph id="ph2">\&lt;</ph>type2&gt;' cannot occur in a constant expression</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The initialization expression in a <ph id="ph1">`Const`</ph> statement evaluates to a data type that cannot be converted to the declared type of the constant.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source><bpt id="p1">**</bpt>Error ID:<ept id="p1">**</ept> BC30060</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>To correct this error</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Initialize the constant with an expression of a data type that can be converted to the type declared for the constant.</source>
        </trans-unit>
