        <trans-unit id="101" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>Attribute '<ph id="ph1">&lt;attributename&gt;</ph>' cannot be applied to a module</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Attribute '<ph id="ph1">\&lt;</ph>attributename&gt;' cannot be applied to a module</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>You attempted to apply an attribute to a module whose <ph id="ph1">`AttributeUsageAttribute`</ph> does not specify <ph id="ph2">`AttributeTargets.Module`</ph>.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>When the attribute was declared, it was not defined to be applied to a module.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source><bpt id="p1">**</bpt>Error ID:<ept id="p1">**</ept> BC30549</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>To correct this error</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Check the attribute declaration and specify <ph id="ph1">`AttributeTargets.Module`</ph> or <ph id="ph2">`AttributeTargets.All`</ph>.</source>
