        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Sets the context (register values) for a thread.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] The operating system-defined thread identifier.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[in] The size of the <ph id="ph1">`pContext`</ph> buffer to be written.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>[in] A pointer to the bytes to be written.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">`SetThreadContext`</ph> method updates the current context for the thread specified by the operating system-defined <ph id="ph2">`dwThreadID`</ph> argument.</source>
        </trans-unit>
