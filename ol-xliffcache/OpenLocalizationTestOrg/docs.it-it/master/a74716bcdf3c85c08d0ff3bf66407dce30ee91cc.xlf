        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source>This element contains all <bpt id="p1">**</bpt><ph id="ph1">\&lt;</ph>setting&gt;<ept id="p1">**</ept> tags that are specific to an instance of the application on a client computer.</source>
        </trans-unit>
