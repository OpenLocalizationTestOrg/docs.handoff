          <source>[in] The native relative virtual address of the method.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[in] The number of local symbols requested.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
