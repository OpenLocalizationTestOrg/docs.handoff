        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>chunkedCookieHandler&gt;</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Configures the <ph id="ph1">&lt;xref:System.IdentityModel.Services.ChunkedCookieHandler&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>This element may only be present if the <ph id="ph1">`mode`</ph> attribute of the <ph id="ph2">`&lt;cookieHandler&gt;`</ph> element is "Default" or "Chunked".</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>system.identityModel.services&gt;</source>
        </trans-unit>
