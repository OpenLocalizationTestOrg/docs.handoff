      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>IMetaDataAssemblyEmit::DefineAssembly Method</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>IMetaDataAssemblyEmit::DefineAssembly Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Creates an <ph id="ph1">`Assembly`</ph> structure containing metadata for the specified assembly and returns the associated metadata token.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] The public key that identifies the publisher of the assembly, or NULL if the assembly is not strongly named.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[in] The size in bytes of <ph id="ph1">`pbPublicKey`</ph>.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>[in] The identifier of the hashing algorithm to use to encrypt the files in the assembly, or NULL to specify the SHA-1 algorithm.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>[in] The human-readable text name of the assembly.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>This value must not exceed 1024 characters.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>[in] A pointer to an ASSEMBLYMETADATA instance that contains the version, platform, and locale information for the assembly.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>[in] A combination of <bpt id="p1">[</bpt>CorAssemblyFlags<ept id="p1">](../../../../docs/framework/unmanaged-api/metadata/corassemblyflags-enumeration.md)</ept> values that describe features of the assembly.</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>[out] A pointer to the metadata token.</source>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Only one <ph id="ph1">`Assembly`</ph> metadata structure can be defined within a manifest.</source>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
