        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Gets the register that contains a variable with a location type of <ph id="ph1">`VLT_REGISTER`</ph>, and the base register for a variable with a location type of <ph id="ph2">`VLT_REGISTER_RELATIVE`</ph>.</source>
        </trans-unit>
