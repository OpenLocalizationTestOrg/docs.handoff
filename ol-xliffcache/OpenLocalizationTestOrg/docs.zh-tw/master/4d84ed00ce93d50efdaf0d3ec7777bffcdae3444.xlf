        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>IHostSyncManager::CreateRWLockReaderEvent Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Creates a manual-reset event object for the implementation of a reader lock.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] <ph id="ph1">`true`</ph>, if <ph id="ph2">`ppEvent`</ph> should be signaled; otherwise, <ph id="ph3">`false`</ph>.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[in] A cookie to associate with the reader lock.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>[out] A pointer to the address of an <bpt id="p1">[</bpt>IHostManualEvent<ept id="p1">](../../../../docs/framework/unmanaged-api/hosting/ihostmanualevent-interface.md)</ept> instance, or null if the event object could not be created.</source>
