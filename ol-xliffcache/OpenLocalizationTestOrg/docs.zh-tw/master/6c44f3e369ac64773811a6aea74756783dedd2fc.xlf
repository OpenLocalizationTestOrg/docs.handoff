      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>ICorDebugNativeFrame::GetLocalMemoryValue Method</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>ICorDebugNativeFrame::GetLocalMemoryValue Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Gets the value of an argument or local variable that is stored in the specified memory location for this native frame.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] A <ph id="ph1">`CORDB_ADDRESS`</ph> value that specifies the memory location containing the value.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[in] An integer that specifies the size of the binary metadata signature which is referenced by the <ph id="ph1">`pvSigBlob`</ph> parameter.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>[in] A <ph id="ph1">`PCCOR_SIGNATURE`</ph> value that points to the binary metadata signature of the value's type.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>[out] A pointer to the address of an "ICorDebugValue" object representing the retrieved value that is stored in the specified memory location.</source>
        </trans-unit>
