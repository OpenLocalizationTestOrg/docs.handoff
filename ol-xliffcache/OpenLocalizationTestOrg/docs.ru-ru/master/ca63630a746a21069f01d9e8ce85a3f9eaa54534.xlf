        <trans-unit id="143" translate="yes" xml:space="preserve">
          <source>Required UI Automation Events</source>
        </trans-unit>
        <trans-unit id="144" translate="yes" xml:space="preserve">
          <source>Menu controls must raise <ph id="ph1">`MenuOpenedEvent`</ph> when they appear on the screen.</source>
        </trans-unit>
        <trans-unit id="145" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">`MenuOpenedEvent`</ph> will include the text of the control.</source>
        </trans-unit>
        <trans-unit id="146" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">`MenuClosedEvent`</ph> must be raised when a menu disappears from the screen.</source>
