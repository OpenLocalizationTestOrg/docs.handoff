          <source><ph id="ph1">&amp;lt;</ph>issuedToken<ph id="ph2">&amp;gt;</ph></source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Specifies a custom token used to authenticate a client to a service.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>system.ServiceModel&gt;</source>
        </trans-unit>
