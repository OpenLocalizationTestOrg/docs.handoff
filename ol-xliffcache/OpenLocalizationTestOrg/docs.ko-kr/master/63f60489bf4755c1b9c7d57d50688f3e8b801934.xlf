        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Gets the value of the specified argument in this Microsoft intermediate language (MSIL) stack frame.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
