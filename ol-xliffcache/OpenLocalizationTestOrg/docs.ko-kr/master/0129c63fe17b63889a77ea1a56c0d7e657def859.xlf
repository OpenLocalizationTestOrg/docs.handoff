          <source><ph id="ph1">\&lt;</ph>customCookieHandler&gt;</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Sets the custom cookie handler type.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>This element may only be present if the <ph id="ph1">`mode`</ph> attribute of the <ph id="ph2">`&lt;cookieHandler&gt;`</ph> element is "Custom".</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
