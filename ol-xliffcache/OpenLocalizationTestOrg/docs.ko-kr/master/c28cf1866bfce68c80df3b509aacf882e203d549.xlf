        <trans-unit id="101" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>'Exit Select' can only appear inside a 'Select' statement</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>'Exit Select' can only appear inside a 'Select' statement</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>An <ph id="ph1">`Exit Select`</ph> statement occurs outside a <ph id="ph2">`Select`</ph> block.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`Exit Select`</ph> is valid only between a <ph id="ph2">`Select`</ph> or <ph id="ph3">`Select Case`</ph> statement and a corresponding <ph id="ph4">`End Select`</ph> statement.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source><bpt id="p1">**</bpt>Error ID:<ept id="p1">**</ept> BC30099</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>To correct this error</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Make sure a valid <ph id="ph1">`Select`</ph> or <ph id="ph2">`Select Case`</ph> statement precedes the <ph id="ph3">`Exit Select`</ph> and a valid <ph id="ph4">`End Select`</ph> statement appears after it.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Verify that other control structures within the <ph id="ph1">`Select`</ph> block are correctly terminated.</source>
