        <trans-unit id="101" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>Reference required to module '<ph id="ph1">&lt;modulename&gt;</ph>' containing the definition for event '<ph id="ph2">&lt;eventname&gt;</ph>'</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Reference required to module '<ph id="ph1">\&lt;</ph>modulename&gt;' containing the definition for event '<ph id="ph2">\&lt;</ph>eventname&gt;'</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Reference required to module '&lt;<ph id="ph1">`modulename`</ph>&gt;' containing the definition for event '&lt;<ph id="ph2">`eventname`</ph>&gt;'.</source>
        </trans-unit>
