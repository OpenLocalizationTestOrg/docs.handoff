        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>IMetaDataEmit::DefineTypeRefByName Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Gets a metadata token for a type that is defined in the specified scope, which is outside the current scope.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] The token specifying the resolution scope.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The following token types are valid:</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`mdModuleRef`</ph>, if the type is defined in the same assembly in which the caller is defined.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`mdAssemblyRef`</ph>, if the type is defined in an assembly other than the one in which the caller is defined.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`mdTypeRef`</ph>, if the type is a nested type.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`mdModule`</ph>, if the type is defined in the same module in which the caller is defined.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Null, if the type is defined globally.</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>[in] The name of the target type in Unicode.</source>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>[out] A pointer to the <ph id="ph1">`mdTypeRef`</ph> token that is assigned to the type.</source>
        </trans-unit>
