        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Adds a <ph id="ph1">`namedCache`</ph> entry to the <ph id="ph2">`namedCaches`</ph> collection for a memory cache.</source>
        </trans-unit>
