        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>certificateReference&gt;</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Specifies settings that are used to find and validate an X.509 certificate in a certificate store.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>system.identityModel.services&gt;</source>
