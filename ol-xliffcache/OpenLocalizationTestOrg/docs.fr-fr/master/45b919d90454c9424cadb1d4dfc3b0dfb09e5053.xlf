          <source>ICorDebugRemote::DebugActiveProcessEx Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Launches a process on a remote machine under the debugger.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] Pointer to an <bpt id="p1">[</bpt>ICorDebugRemoteTarget Interface<ept id="p1">](../../../../docs/framework/unmanaged-api/debugging/icordebugremotetarget-interface.md)</ept>.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>This parameter is used to determine the machine on which the process is running.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>[in] The ID of the process to which the debugger is to be attached.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>[in] <ph id="ph1">`true`</ph> if the debugger should behave as the Win32 debugger for the process and dispatch the unmanaged callbacks; otherwise, <ph id="ph2">`false`</ph>.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>[out] A pointer to the address of an "ICorDebugProcess" object that represents the process to which the debugger has been attached.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Return Value</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>S_OK</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>Successfully attached to the process on the remote machine.</source>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>E_FAIL (or other E_ return codes)</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Unable to attach to the process on the remote machine.</source>
        </trans-unit>
