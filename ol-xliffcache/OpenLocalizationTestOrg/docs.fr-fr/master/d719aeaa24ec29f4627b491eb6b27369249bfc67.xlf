        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>idleTimeout</source>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>A TimeSpan value that specifies the maximum interval of time for which the object can remain idle in the cache before being disposed.</source>
        </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>leaseTimeout</source>
        </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source>A TimeSpan value that specifies  the interval of time after which an object is removed from the cache.</source>
        </trans-unit>
