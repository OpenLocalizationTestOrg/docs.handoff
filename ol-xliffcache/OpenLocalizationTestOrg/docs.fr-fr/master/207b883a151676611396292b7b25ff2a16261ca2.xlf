        <trans-unit id="151" translate="yes" xml:space="preserve">
          <source>Description</source>
        </trans-unit>
        <trans-unit id="152" translate="yes" xml:space="preserve">
          <source>Enumeration</source>
        </trans-unit>
        <trans-unit id="153" translate="yes" xml:space="preserve">
          <source>One of the following values: LocalMachine or CurrentUser.</source>
        </trans-unit>
        <trans-unit id="154" translate="yes" xml:space="preserve">
          <source>The default is CurrentUser.</source>
        </trans-unit>
        <trans-unit id="155" translate="yes" xml:space="preserve">
          <source>If the client application is running under a system account, then the certificate is typically under LocalMachine.</source>
        </trans-unit>
        <trans-unit id="156" translate="yes" xml:space="preserve">
          <source>If the client application is running under a user account, then the certificate is typically in CurrentUser.</source>
        </trans-unit>
