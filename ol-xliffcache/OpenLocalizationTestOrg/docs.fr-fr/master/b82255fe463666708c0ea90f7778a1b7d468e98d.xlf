        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>ICorDebugRegisterSet::GetRegistersAvailable Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Gets a bit mask indicating which registers in this <bpt id="p1">[</bpt>ICorDebugRegisterSet<ept id="p1">](../../../../docs/framework/unmanaged-api/debugging/icordebugregisterset-interface.md)</ept> are currently available.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[out] A bit mask that indicates which registers are currently available.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>A register may be unavailable if its value cannot be determined for the given situation.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>The returned mask contains a bit for each register (1 &lt;&lt; the register index).</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The bit value is 1 if the register is available, or 0 if it is not available.</source>
        </trans-unit>
