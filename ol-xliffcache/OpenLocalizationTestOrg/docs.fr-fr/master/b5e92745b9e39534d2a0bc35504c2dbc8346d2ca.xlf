        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>windows&gt; of <ph id="ph2">\&lt;</ph>clientCredentials&gt; Element</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Specifies the settings for a Windows credential to be used to represent the client.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>system.ServiceModel&gt;</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>behaviors&gt;</source>
        </trans-unit>
