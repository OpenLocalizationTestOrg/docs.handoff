        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>To span columns and rows</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Drag a <ph id="ph1">&lt;xref:System.Windows.Forms.TableLayoutPanel&gt;</ph> control from the <bpt id="p1">**</bpt>Toolbox<ept id="p1">**</ept> onto your form.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Drag a <ph id="ph1">&lt;xref:System.Windows.Forms.Button&gt;</ph> control from the <bpt id="p1">**</bpt>Toolbox<ept id="p1">**</ept> into the upper-left cell of the <ph id="ph2">&lt;xref:System.Windows.Forms.TableLayoutPanel&gt;</ph> control.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Set the <ph id="ph1">&lt;xref:System.Windows.Forms.Button&gt;</ph> control's <bpt id="p1">**</bpt>ColumnSpan<ept id="p1">**</ept> property to <bpt id="p2">**</bpt>2<ept id="p2">**</ept>.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Note that the <ph id="ph1">&lt;xref:System.Windows.Forms.Button&gt;</ph> control spans the first and second columns.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Set the <ph id="ph1">&lt;xref:System.Windows.Forms.Button&gt;</ph> control's <bpt id="p1">**</bpt>RowSpan<ept id="p1">**</ept> property to <bpt id="p2">**</bpt>2<ept id="p2">**</ept>.</source>
