        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The number of bytes of memory allocated on the thread that is associated with the current <ph id="ph1">`COR_GC_THREAD_STATS`</ph> instance.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
