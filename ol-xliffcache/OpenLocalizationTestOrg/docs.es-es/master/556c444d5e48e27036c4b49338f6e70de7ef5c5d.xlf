        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>audienceUris&gt;</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Specifies the set of URIs that are acceptable identifiers of the relying party (RP).</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Tokens will not be accepted unless they are scoped for one of the allowed audience URIs.</source>
        </trans-unit>
