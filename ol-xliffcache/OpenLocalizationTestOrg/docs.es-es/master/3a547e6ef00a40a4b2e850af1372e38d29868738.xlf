        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>ICorDebugNativeFrame::GetLocalRegisterValue Method | Microsoft Docs</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>ICorDebugNativeFrame::GetLocalRegisterValue Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Gets the value of an argument or local variable that is stored in the specified register for this native frame.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] A value of the "CorDebugRegister" enumeration that specifies the register containing the value.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[in] An integer that specifies the size of the binary metadata signature which is referenced by the <ph id="ph1">`pvSigBlob`</ph> parameter.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>[in] A <ph id="ph1">`PCCOR_SIGNATURE`</ph> value that points to the binary metadata signature of the value's type.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>[out] A pointer to the address of an "ICorDebugValue" object representing the retrieved value that is stored in the specified register.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">`GetLocalRegisterValue`</ph> method can be used either in a native frame or a just-in-time (JIT)-compiled frame.</source>
        </trans-unit>
