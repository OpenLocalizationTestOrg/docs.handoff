          <source>Parts</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Required.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>An <ph id="ph1">&lt;xref:System.Xml.Linq.XElement&gt;</ph> object, an <ph id="ph2">&lt;xref:System.Xml.Linq.XDocument&gt;</ph> object, a collection of <ph id="ph3">&lt;xref:System.Xml.Linq.XElement&gt;</ph> objects, or a collection of <ph id="ph4">&lt;xref:System.Xml.Linq.XDocument&gt;</ph> objects.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>...&lt;</source>
        </trans-unit>
