          <source>See Also</source>
        </trans-unit>
        <trans-unit id="185" translate="yes" xml:space="preserve">
          <source><ph id="ph1">&lt;xref:System.Windows.Forms.MaskedTextBox.Mask%2A&gt;</ph></source>
        </trans-unit>
