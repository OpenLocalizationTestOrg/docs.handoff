        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>By default, a real numeric literal on the right side of the assignment operator is treated as <bpt id="p1">[</bpt>double<ept id="p1">](double.md)</ept>.</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
