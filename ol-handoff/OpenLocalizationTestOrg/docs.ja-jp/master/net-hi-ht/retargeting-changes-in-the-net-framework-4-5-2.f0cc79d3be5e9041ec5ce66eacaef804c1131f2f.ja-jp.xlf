        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Retargeting Changes in the .NET Framework 4.5.2</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>In rare cases, retargeting changes may affect apps that are recompiled to target the .NET Framework 4.5.2.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Unless specified otherwise in the following tables, these changes do not affect binaries that target a previous version of the .NET Framework but are running under version 4.5.2.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The .NET Framework 4.5.2 includes retargeting changes in the following areas:</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source><bpt id="p1">[</bpt>Entity Framework<ept id="p1">](#EF)</ept></source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source><bpt id="p1">[</bpt>Windows Forms<ept id="p1">](#WinForms)</ept></source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Entity Framework retargeting changes</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Feature</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
