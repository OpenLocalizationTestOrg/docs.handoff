          <source><ph id="ph1">`/n &lt;`</ph> <bpt id="p1">*</bpt>subjectName<ept id="p1">*</ept> <ph id="ph2">`&gt;`</ph></source>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>Specifies the subject name of the certificate.</source>
        </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
