        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Gets the set of properties of the manifest resource with the specified metadata signature.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] An <ph id="ph1">`mdManifestResource`</ph> token that represents the resource for which to get the properties.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[out] The name of the resource.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>[in] The size, in wide chars, of <ph id="ph1">`szName`</ph>.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>[out] A pointer to the number of wide chars actually returned in <ph id="ph1">`szName`</ph>.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>[out] A pointer to an <ph id="ph1">`mdFile`</ph> token or an <ph id="ph2">`mdAssemblyRef`</ph> token that represents the file or assembly, respectively, that contains the resource.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>[out] A pointer to a value that specifies the offset to the beginning of the resource within the file.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>[out] A pointer to flags that describe the metadata applied to a resource.</source>
        </trans-unit>
