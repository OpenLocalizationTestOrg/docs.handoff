      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" restype="x-metadata">
          <source>ICorDebugController::SetAllThreadsDebugState Method | Microsoft Docs</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>ICorDebugController::SetAllThreadsDebugState Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Sets the debug state of all managed threads in the process.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] A value of the "CorDebugThreadState" enumeration that specifies the state of the thread for debugging.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[in] A pointer to an "ICorDebugThread" object that represents a thread to be exempted from the debug state setting.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>If this value is null, no thread is exempted.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Remarks</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">`SetAllThreadsDebugState`</ph> method may affect threads that are not visible via <bpt id="p1">[</bpt>EnumerateThreads Method<ept id="p1">](../../../../docs/framework/unmanaged-api/debugging/icordebugcontroller-enumeratethreads-method.md)</ept>, so threads that were suspended with the <ph id="ph2">`SetAllThreadsDebugState`</ph> method will need to be resumed with the <ph id="ph3">`SetAllThreadsDebugState`</ph> method.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
