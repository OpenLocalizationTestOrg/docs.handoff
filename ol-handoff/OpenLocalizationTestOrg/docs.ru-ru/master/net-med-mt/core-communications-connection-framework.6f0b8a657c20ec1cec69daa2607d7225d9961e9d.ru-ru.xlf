        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>This topic lists all exceptions generated by <ph id="ph1">[!INCLUDE[indigo1](../../../../../includes/indigo1-md.md)]</ph> Connection Framework.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
