          <source>persistableTypes</source>
        </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>All the persistable types.</source>
        </trans-unit>
        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>userDefinedTypes</source>
        </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source>A collection of User Defined Types (UDT) that is to be included in the service contract.</source>
        </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>exposedMethods</source>
        </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
