        <trans-unit id="140" translate="yes" xml:space="preserve">
          <source>Configuration file</source>
        </trans-unit>
        <trans-unit id="141" translate="yes" xml:space="preserve">
          <source>This element can be used in the application configuration file, machine configuration file (<bpt id="p1">*</bpt>Machine.config<ept id="p1">*</ept>), and <bpt id="p2">*</bpt>Web.config<ept id="p2">*</ept> files that are not at the application directory level.</source>
        </trans-unit>
