          <source><ph id="ph1">&amp;lt;</ph>channelPoolSettings<ph id="ph2">&amp;gt;</ph></source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Specifies the channel pool settings for a custom binding.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>system.serviceModel&gt;</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source><ph id="ph1">\&lt;</ph>bindings&gt;</source>
        </trans-unit>
