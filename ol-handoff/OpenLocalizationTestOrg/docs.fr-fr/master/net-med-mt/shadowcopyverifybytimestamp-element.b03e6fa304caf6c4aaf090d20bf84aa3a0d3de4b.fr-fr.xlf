        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source><ph id="ph1">&amp;lt;</ph>shadowCopyVerifyByTimestamp<ph id="ph2">&amp;gt;</ph> Element | Microsoft Docs</source>
