        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>IMetaDataEmit::DefineField Method</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Creates a definition for a field with the specified metadata signature, and gets a token to that field definition.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Parameters</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>[in] The <ph id="ph1">`mdTypeDef`</ph> token for the enclosing class or interface.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>[in] The field name in Unicode.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>[in] The field attributes.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>This is a bitmask of <ph id="ph1">`CorFieldAttr`</ph> values.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>[in] The field signature as a BLOB.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>[in] The count of bytes in <ph id="ph1">`pvSigBlob`</ph>.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>[in] The <ph id="ph1">`ELEMENT_TYPE_`</ph><bpt id="p1">*</bpt><ph id="ph2">\*</ph><ept id="p1">*</ept> for the constant value.</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>This is a <ph id="ph1">`CorElementType`</ph> value.</source>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>If not defining a constant value for the field, use <ph id="ph1">`ELEMENT_TYPE_END`</ph>.</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>[in] The constant value for the field.</source>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>[in] The size in (Unicode) characters of <ph id="ph1">`pValue`</ph>.</source>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>[out] The <ph id="ph1">`mdFieldDef`</ph> token assigned.</source>
        </trans-unit>
