          <source>C# 6</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source><bpt id="p1">[</bpt>Constructor<ept id="p1">](#constructors)</ept></source>
        </trans-unit>
