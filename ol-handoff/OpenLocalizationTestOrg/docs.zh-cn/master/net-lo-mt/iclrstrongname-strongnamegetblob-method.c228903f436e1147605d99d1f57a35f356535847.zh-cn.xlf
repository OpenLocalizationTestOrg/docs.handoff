        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Fills the specified buffer with the binary representation of the executable file at the specified address.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
        </trans-unit>
